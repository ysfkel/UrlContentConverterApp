 <div class="panel panel-default" ng-controller="ctr as vm">
  <div class="panel-body">

        <form action="" novalidate ng-submit="vm.actions.getHash($event)">
            <div class="row">
                <div class="col-md-8">
                    <input type="text" ng-model="vm.model.Url" class="form-control" placeholder="Please enter url in format http://www.example.com">
                </div>
                <div class="col-md-4">
                       <input type="submit" value="Get HttpContent Hash" class="btn btn-primary"/>
                </div>
            </div>
            <div >
            
            </div>
        </form>
     <hr>
    <header ng>
        <h2>{{vm.resultTitle}}</h2>
    </header>
     <section>
         <p>
        {{vm.result}}
         </p>
     </section>
  </div>
</div>
